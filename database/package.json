{
  "name": "@hookah-db/database",
  "version": "1.0.0",
  "description": "Database schemas and migrations for hookah tobacco database",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "migrate": "node dist/migrate.js",
    "seed": "node dist/seed.js"
  },
  "dependencies": {
    "@hookah-db/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
