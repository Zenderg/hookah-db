# Development Environment Variables Template
# Copy this file to .env.dev and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://hookah_user:hookah_password@localhost:5432/hookah_db
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10
